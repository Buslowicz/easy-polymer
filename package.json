{
  "name": "easy-polymer",
  "version": "0.1.3",
  "description": "EaSy Polymer is a set of ES7/TS decorators for Polymer to save on boilerplate",
  "main": "es5/index.js",
  "jsnext:main": "es6/index.js",
  "types": "index.d.ts",
  "author": "Draccoz",
  "license": "ISC",
  "keywords": [
    "polymer",
    "decorator",
    "typescript",
    "es7",
    "es2016"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Draccoz/easy-polymer"
  },
  "homepage": "https://github.com/Draccoz/easy-polymer",
  "scripts": {
    "build": "gulp build",
    "prepublish": "npm run build"
  },
  "config": {
    "app": {
      "entry": "index",
      "src": "./",
      "dist": "./",
      "formats": [
        "es",
        "iife",
        "iife:min"
      ]
    }
  },
  "devDependencies": {
    "gulp": "latest",
    "gulp-empty": "^0.1.1",
    "gulp-rename": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-tslint": "latest",
    "gulp-wrapper": "latest",
    "lodash": "^4.16.6",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-stream": "^1.14.0",
    "tslint": "^3.15.1",
    "tslint-config-standard": "^1.5.0",
    "typescript": "^2.0.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "files": [
    "*.js",
    "*.ts",
    "*.json",
    "*.html",
    "*.map"
  ]
}
